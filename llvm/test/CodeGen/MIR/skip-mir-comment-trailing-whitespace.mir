# RUN: llc %s -mtriple=riscv64 -mattr=+v -run-pass=none -o - | FileCheck %s

---
name:            test_vse8
tracksRegLiveness: true
body:             |
  bb.0:
    ; CHECK-LABEL: name: test_vse8{{$}}
    liveins: $x10, $v8, $x11

    %2:gprnox0 = COPY $x11
    %1:vr = COPY $v8
    %0:gpr = COPY $x10
    PseudoVSE8_V_MF8 %1, %0, %2, 3 /* e8 */ :: (store unknown-size, align 1)
    PseudoRET
...
