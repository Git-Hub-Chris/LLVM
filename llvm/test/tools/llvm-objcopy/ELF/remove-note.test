--- !ELF
FileHeader:
  Class:          ELFCLASS64
  Data:           ELFDATA2LSB
  Type:           ET_CORE
  Machine:        EM_X86_64
ProgramHeaders:
  - Type:         PT_NOTE
    MemSize:      0
    FirstSec:     .note
    LastSec:      .note
Sections:
  - Name:         .note
    Type:         SHT_NOTE
    AddressAlign: [[ALIGN]]
    Notes:
      - Name:   CORE
        Type:   0x01
        Desc:   0101
      - Name:   CORE
        Type:   0x02
        Desc:   0202
      - Name:   CORE
        Type:   0x03
        Desc:   0303
      - Name:   CORE
        Type:   0x04
        Desc:   0404
      - Name:   LINUX
        Type:   0x01
        Desc:   0505
...
